# Environment Variables for Neo-Brutalism Todo App

# App Configuration
VITE_APP_NAME=Neo Brutalism Todo
VITE_APP_VERSION=1.0.0

# Development
VITE_DEV_MODE=true

# API Configuration (if needed for future features)
# VITE_API_BASE_URL=http://localhost:3001/api

# Theme Configuration
VITE_DEFAULT_THEME=light

# Local Storage Keys
VITE_STORAGE_PREFIX=neo-brutalism-todo

# Animation Settings
VITE_ENABLE_ANIMATIONS=true
VITE_ANIMATION_DURATION=300

# Debug Mode
VITE_DEBUG=false